	function EntityActionWait(entityObject, time)
		
		while time > 0 do
			Program.Wait(0.1);
			time = time - 0.1;
	
			if EntityController.IsActionCancelled(entityObject) then
				return false;
			end
		end
		
		return true;
	end
	

